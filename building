class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class linkedlist:
    def __init__(self):
        self.head = None

    def insert_sorter(self, data, index):
        new_node = Node(data)
        current_node = self.head
        position = 0 
        if position == index:
            self.insert_sorter(data)
